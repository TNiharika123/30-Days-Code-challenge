//C

#include <stdio.h> // header file for Standard Input Output
#include <stdlib.h> // header file for Standard Library

int main() {
    int S,T,a,b,c,count=0;

    scanf("%d %d",&S,&T);
    for(a=0;a<=S;a++)
    {
	   for(b=0;b<=S;b++)
       {
	     for(c=0;c<=S;c++)
         { if(a+b+c<=S && a*b*c<=T)
            {// printf("\n%d %d %d",a,b,c);
              count++;
     
     
            }
         }
       }
     }
     printf("%d",count);
    return 0;
}
