//C++

#include <bits/stdc++.h> // header file includes every Standard library
using namespace std;

int main() {
	string s,t;
    cin>>s>>t;
    int count=0;
    for(int i=0; i<s.length()-1; i++)
    { if(s[i]== t[i] || s[i]==t[i+1])
        {
           count++;
        }
    }
    if(count == s.length()-1)
    {
        cout<<"Yes";
    }else
    {
        cout<<"No";
    }
    return 0;
}
